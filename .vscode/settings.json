{
    "json.schemas": [
        {
            "fileMatch": ["CustomNPC/data/Chars/*/config.json"],
            "url": "./CustomNPC/schema/CharConfig.schema.json"
        },
        {
            "fileMatch": [
                "CustomNPC/data/Chars/*/StaticData/**/*.json",
                "CustomNPC/data/StaticData/**/*.json"
            ],
            "url": "./Schema/schema/AnyCddaJsonList.schema.json"
        },
        {
            "fileMatch": [
                "SmartNPC/data/CastAI/**/*.json"
            ],
            "url": "./SmartNPC/schema/CastAIDataJsonTable.schema.json"
        },
        {
            "fileMatch": ["CustomMedia/data/Media/*/image/*.json"],
            "url": "./CustomMedia/schema/ImageInfo.schema.json"
        },
        {
            "fileMatch": ["CustomMedia/data/Media/*/anime/info.json"],
            "url": "./CustomMedia/schema/AnimeInfo.schema.json"
        },
    ],
    "js/ts.implicitProjectConfig.target": "ES2022",
    "js/ts.implicitProjectConfig.module": "CommonJS",
    "typescript.tsdk": "./node_modules/typescript/lib"
}