{
    "json.schemas": [
        {
            "fileMatch": ["CDDA-CustomNPC/data/chars/*/config.json"],
            "url": "./CDDA-CustomNPC/schema/CharConfig.schema.json"
        },
        {
            "fileMatch": ["CDDA-CustomNPC/data/chars/*/image/*.json"],
            "url": "./CDDA-CustomNPC/schema/ImageInfo.schema.json"
        },
        {
            "fileMatch": ["CDDA-CustomNPC/data/chars/*/anime/info.json"],
            "url": "./CDDA-CustomNPC/schema/AnimeInfo.schema.json"
        },
        {
            "fileMatch": [
                "CDDA-CustomNPC/data/chars/*/StaticData/**/*.json",
                "CDDA-CustomNPC/data/StaticData/**/*.json"
            ],
            "url": "./CDDA-Schema/schema/AnyCddaJsonList.schema.json"
        },
        {
            "fileMatch": [
                "CDDA-SmartNPC/data/CastAI/**/*.json"
            ],
            "url": "./CDDA-SmartNPC/schema/AIDataTable.schema.json"
        }
    ]
}